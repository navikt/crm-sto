<aura:component implements="lightning:backgroundUtilityItem, flexipage:availableForAllPageTypes" access="global"
    controller="stoUtilityBackgroundController">
    <lightning:omniToolkitAPI aura:id="omniToolkit" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <aura:handler event="lightning:tabCreated" action="{! c.onTabCreated }" />
    <aura:handler event="lightning:omniChannelWorkAccepted" action="{! c.onWorkAccepted }"/>
    <lightning:workspaceAPI aura:id="workspace" />
    <lightning:flow aura:id="ChangeOwner" onstatuschange="{!c.handleStatusChange}"/>
</aura:component>