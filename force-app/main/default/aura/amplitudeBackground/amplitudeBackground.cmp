<aura:component implements="flexipage:availableForAllPageTypes" controller="AmplitudeHelper">
    <ltng:require scripts="{!$Resource.Amplitude + '/Amplitude.js'}" />
    <c:nksGetStoUtility aura:id="stoUtility"></c:nksGetStoUtility>
    <c:amplitude aura:id="amplitude" />

    <aura:attribute name="recordIdMap" type="Object" />

    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />

    <lightning:workspaceAPI aura:id="workspace" />
    <aura:handler event="lightning:tabFocused" action="{! c.onTabFocused }" />
    <lightning:messageChannel type="amplitude__c" onMessage="{!c.logMessage}" scope="APPLICATION" />
</aura:component>
