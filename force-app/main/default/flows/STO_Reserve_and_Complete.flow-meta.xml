    <decisions>
        <name>Already_Complete</name>
        <label>Already Complete?</label>
        <locationX>446</locationX>
        <locationY>758</locationY>
        <defaultConnector>
            <targetReference>Assign_On_Hold_status</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Already_Completed</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>caseToUpdate.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>On Hold</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>caseToUpdate.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Closed</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Case_Completed_Screen</targetReference>
            </connector>
            <label>Already Completed</label>
        </rules>
    </decisions>
